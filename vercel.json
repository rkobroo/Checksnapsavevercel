
{
  "version": 2,
  "builds": [
    {
      "src": "preview/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/preview/server.js"
    },
    {
      "src": "/health",
      "dest": "/preview/server.js"
    },
    {
      "src": "/status",
      "dest": "/preview/server.js"
    },
    {
      "src": "/info",
      "dest": "/preview/server.js"
    },
    {
      "src": "/download",
      "dest": "/preview/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/preview/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
